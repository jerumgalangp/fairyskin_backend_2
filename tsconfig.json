{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": ["dom", "es6", "es2017", "esnext.asynciterable"],
        "sourceMap": true,
        "outDir": "api-build",
        "moduleResolution": "node",
        "removeComments": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "skipLibCheck": true /* Skip type checking of declaration files. */,
        "baseUrl": "."
    },
    "typeRoots": ["./source/@types", "./node_modules/@types"],
    "exclude": ["node_modules"],
    "include": [
        "./source/**/*.ts",
        "source/entities/HolidayEntities.ts_remove",
        "source/entities/EWTEntities.ts_remove",
        "source/dao/EWTDao.ts_remove",
        "source/dao/HolidayDao.ts_remove",
        "source/services/EWTService.ts_remove",
        "source/services/HolidayService.ts_remove",
        "source/controllers/EWTController.ts_remove",
        "source/controllers/HolidayController.ts_remove",
        "source/routes/EWTRouter.ts_remove",
        "source/entities/PersonnelEntities.ts_remove",
        "source/dao/PersonnelDao.ts_remove",
        "source/controllers/PersonnelController.ts_remove",
        "source/routes/PersonnelRouter.ts_remove",
        "source/services/PersonnelService.ts_remove"
    ]
}
